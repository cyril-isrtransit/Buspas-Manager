@model  IEnumerable<SivMsgWebClient.PIS_Tickers>
    @{
        ViewBag.Title = Resources.LanguageResource.DashBoard;
    }


    <!-- begin #content -->
    <div id="content" class="content">
        <!-- begin breadcrumb -->
        <ol class="breadcrumb pull-right">
            <li>@Resources.LanguageResource.Home</li>
            <li class="active">@Resources.LanguageResource.DashBoard </li>
        </ol>
        <!-- end breadcrumb -->
        <!-- begin page-header -->
        <h1 class="page-header">@Resources.LanguageResource.DashBoard <small></small></h1>
        <!-- end page-header -->
        <!-- begin row -->
        <div class="row" id="statsRow">
            <div class="col-md-3">
                <!-- begin widget -->
                <div class="widget widget-stat bg-purple text-white" id="widget-stat-inactive">
                    <div class="widget-stat-icon"><i class="fa fa-clock-o"></i></div>
                    <div class="widget-stat-info">
                        <div class="widget-stat-title">@Resources.LanguageResource.InactiveMessagesInScope</div>
                        <div class="widget-stat-number" id="InactiveMessagesInScope"></div>
                        <div class="widget-stat-text" id="InactiveMessagesInScopepc"></div>
                    </div>
                </div>
                <!-- end widget -->
            </div>
            <div class="col-md-3">
                <!-- begin widget -->
                <div class="widget widget-stat bg-primary text-white" id="widget-stat-future">
                    <div class="widget-stat-icon"><i class="fa fa-eye-slash"></i></div>
                    <div class="widget-stat-info">
                        <div class="widget-stat-title">@Resources.LanguageResource.InactiveMessagesInFuture</div>
                        <div class="widget-stat-number" id="InactiveMessagesInFutureScope"></div>
                        <div class="widget-stat-text" id="InactiveMessagesInFutureScopepc"></div>
                    </div>
                </div>
                <!-- end widget -->
            </div>
            <div class="col-md-3">
                <!-- begin widget -->
                <div class="widget widget-stat bg-danger text-white" id="widget-stat-outdated">
                    <div class="widget-stat-icon"><i class="fa fa-times"></i></div>
                    <div class="widget-stat-info">
                        <div class="widget-stat-title">@Resources.LanguageResource.ActiveOutdatedMessages</div>
                        <div class="widget-stat-number" id="ActiveOutdatedMessages"></div>
                        <div class="widget-stat-text" id="ActiveOutdatedMessagespc"></div>
                    </div>
                </div>
                <!-- end widget -->
            </div>
            <div class="col-md-3">
                <!-- begin widget -->
                <div class="widget widget-stat bg-success text-white" id="widget-stat-active">
                    <div class="widget-stat-icon"><i class="fa fa-check"></i></div>
                    <div class="widget-stat-info">
                        <div class="widget-stat-title">@Resources.LanguageResource.ActiveMessagesInScope</div>
                        <div class="widget-stat-number" id="ActiveMessagesInScope"></div>
                        <div class="widget-stat-text" id="ActiveMessagesInScopepc"></div>
                    </div>
                </div>
                <!-- end widget -->
            </div>
        </div>


        <div class="row m-b-20">
            <div class="col-md-12" id="calendarmessages">
                <!-- begin panel -->
                <div class="panel pagination-inverse clearfix m-b-0 panel-inverse">
                    <div class="panel-heading">
                        <div class="panel-heading-btn">
                            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-lime" data-click="panel-expand"><i class="fa fa-expand"></i></a>
                            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-warning" data-click="panel-collapse"><i class="fa fa-minus"></i></a>
                            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-danger" data-click="panel-remove"><i class="fa fa-times"></i></a>
                        </div>
                        <div class="btn-group">
                            <div class="btn-xs btn-default"><b>@Resources.LanguageResource.Schedule</b></div>
                        </div>
                    </div>
                    <div class="panel-body p-b-0">
                        <!-- begin fullcalendar -->
                        <div class="fullcalendar vertical-box m-b-20">
                            <!-- begin fullcalendar-event -->
                            <div class="fullcalendar-event vertical-box-column hidden-sm hidden-xs">
                                <div id="external-events">
                                    <h5 class="m-t-0 m-b-10">@Resources.LanguageResource.Status</h5>
                                    <div class="fc-event-icon"><i class="fa fa-circle-o fa-fw text-lime-light"></i> @Resources.LanguageResource.Active</div>
                                    <div class="fc-event-icon"><i class="fa fa-circle-o fa-fw text-danger-light"></i> @Resources.LanguageResource.Inactive</div>
                                </div>
                            </div>
                            <!-- end fullcalendar-event -->
                            <!-- begin fullcalendar-container -->
                            <div class="fullcalendar-container vertical-box-column">
                                <div id='calendar'></div>
                            </div>
                            <!-- end fullcalendar-container -->
                        </div>
                        <!-- end panel -->
                    </div>
                </div>
            </div>
        </div>

        <div class="row m-b-20">
            <div class="col-md-12" id="currentmessages">
                <!-- begin panel -->
                <div class="panel pagination-inverse clearfix m-b-0 panel-inverse">
                    <div class="panel-heading">
                        <div class="panel-heading-btn">
                            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-lime" data-click="panel-expand"><i class="fa fa-expand"></i></a>
                            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-warning" data-click="panel-collapse"><i class="fa fa-minus"></i></a>
                            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-danger" data-click="panel-remove"><i class="fa fa-times"></i></a>
                        </div>
                        <div class="btn-group">
                            <div class="btn-xs btn-default"><b>@Resources.LanguageResource.CurrentMessages</b></div>
                        </div>
                    </div>
                    <div class="panel-body p-b-0">
                        <div class="table-responsive">
                            <table id="data-table" data-order='[[0,"asc"]]' class="table table-bordered table-hover table-td-valign-middle table-striped">
                                <thead>
                                    <tr class="">
                                        <th>@Resources.LanguageResource.Id</th>
                                        <th>@Resources.LanguageResource._Text</th>
                                        <th>@Resources.LanguageResource.StartDate</th>
                                        <th>@Resources.LanguageResource.EndDate</th>
                                        <th>@Resources.LanguageResource.Status</th>
                                        <th>@Resources.LanguageResource.Color</th>
                                        <th>@Resources.LanguageResource.Link</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr class="odd gradeX">
                                            <td>@item.Ticker_ID</td>
                                            <td>@item.Ticker_Text</td>
                                            <td>@item.Start_Time</td>
                                            <td>@item.Finish_Time</td>
                                            @if (item.IS_Valid.ToString() == "False")
                                            {
                                                <td><div class="statusTd"><span class="label label-danger">@Resources.LanguageResource.Inactive</span></div></td>
                                            }
                                            else
                                            {
                                                <td><div class="statusTd"><span class="label label-success">@Resources.LanguageResource.Active</span></div></td>
                                            }
                                            <td>@item.Tiker_Color</td>
                                            <td>@item.Tiker_URL</td>
                                        </tr>
                                    }
                            </table>
                        </div>
                    </div>
                </div>
                <!-- end panel -->
            </div>
        </div>

        <!-- begin #footer -->
        <div id="footer" class="footer">
            <span class="pull-right">
                <a href="javascript:;" class="btn-scroll-to-top" data-click="scroll-top">
                    <i class="fa fa-arrow-up"></i> <span class="hidden-xs">@Resources.LanguageResource.BackToTop</span>
                </a>
            </span>
            &copy; 2016 <b>ISR Transit</b> @Resources.LanguageResource.AllRightReserved
        </div>
        <!-- end #footer -->
    </div>



    <!-- #Calendar-dialog -->
    <div class="modal fade" id="eventContent">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                    <h4 class="modal-title"><span>@Resources.LanguageResource.Messages</span></h4>
                </div>
                <div class="modal-body">
                    <div class="form-group m-b-10 row">
                        <label class="col-md-3 control-label">@Resources.LanguageResource.Id : </label>
                        <div class="col-md-9">
                            <span id="Ticker_ID"></span>
                        </div>
                    </div>
                    <div class="form-group m-b-10 row">
                        <label class="col-md-3 control-label">@Resources.LanguageResource.StartDate : </label>
                        <div class="col-md-9">
                            <span id="startTime"></span>
                        </div>
                    </div>
                    <div class="form-group m-b-10 row">
                        <label class="col-md-3 control-label">@Resources.LanguageResource.EndDate : </label>
                        <div class="col-md-9">
                            <span id="endTime"></span>
                        </div>
                    </div>
                    <div class="form-group m-b-10 row">
                        <label class="col-md-3 control-label">@Resources.LanguageResource._Text : </label>
                        <div class="col-md-9">
                            <span id="text"></span>
                        </div>
                    </div>
                    <div class="form-group m-b-10 row">
                        <label class="col-md-3 control-label">@Resources.LanguageResource.Status : </label>
                        <div class="col-md-9">
                            <span id="eventStatus"></span>
                        </div>
                    </div>
                    <div class="form-group m-b-10 row">
                        <label class="col-md-3 control-label">@Resources.LanguageResource.Color : </label>
                        <div class="col-md-9">
                            <span id="color"></span>
                        </div>
                    </div>
                    <div class="form-group m-b-10 row">
                        <label class="col-md-3 control-label">@Resources.LanguageResource.Link : </label>
                        <div class="col-md-9">
                            <span id="link"></span>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <a href="javascript:;" class="btn btn-sm width-100 btn-default" data-dismiss="modal">@Resources.LanguageResource.Close</a>
                </div>
            </div>
        </div>
    </div>
